      "elements": [
        {
          "type": "title",
          "content": "The Light-Independent Reactions of Photosynthesis",
          "position": {
            "left": 0.8,
            "top": 0.6,
            "width": 11.73,
            "height": 0.7
          },
          "style": {
            "font_size": 32,
